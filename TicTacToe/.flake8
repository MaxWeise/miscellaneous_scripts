[flake8]
exclude =
    .git,
    __pycache__,
    docs/
    __init__.py
ignore = 
    #W391: Blank lines at end of file
    W391
    # E401: Multiple imports one one line
    E401
    # E402: module level import not at top of line
    E402
    # E126: overindentation
    E126
max-complexity = 9 
max-line-length = 100
